{
    "package": {
        "package_name": "net.intellivoid.deepanalytics",
        "name": "DeepAnalytics",
        "version": "1.0.0.3",
        "author": "Zi Xing Narrakas",
        "organization": "Intellivoid",
        "description": "Advanced & Fast Analytics using MongoDB",
        "url": "https://github.com/intellivoid/DeepAnalytics",
        "dependencies": [
            {
                "package": "net.intellivoid.acm",
                "version": "latest",
                "source": "default@github/intellivoid/acm",
                "required": true
            },
            {
                "package": "com.mongodb.mongodb",
                "version": "latest",
                "source": "mongodb@composer/mongodb",
                "required": true
            }
        ],
        "configuration": {
            "autoload_method": "generated_spl",
            "main": null,
            "post_installation": [],
            "pre_installation": []
        }
    },
    "components": [
        {
            "required": true,
            "file": "DeepAnalytics.php"
        },
        {
            "required": true,
            "file": "Utilities.php"
        },
        {
            "required": true,
            "file": "Exceptions/DataNotFoundException.php"
        },
        {
            "required": true,
            "file": "AutoConfig.php"
        },
        {
            "required": true,
            "file": "Objects/Date.php"
        },
        {
            "required": true,
            "file": "Objects/HourlyData.php"
        },
        {
            "required": true,
            "file": "Objects/MonthlyData.php"
        }
    ],
    "files": [
        "package.json",
        "configuration.ini",
        "deep_analytics.json"
    ]
}